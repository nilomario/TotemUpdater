﻿@model Passline.Totem.Core.DataTransferObjects.ProductoDto
<div style="margin-top:10px; margin-bottom:10px"></div>
<div class="table-responsive box box-body box-primary" style="padding: 20px !important">
    <table class="table nowrap table-hover" id="dataTable">
        <thead>
            <tr style="background-color:#3d566e; color:#ecf0f1">
                <th>
                    @Html.LabelFor(model => model.nombre)
                </th>
                <th>
                    @Html.LabelFor(model => model.descripcion)
                </th>
                <th>
                    @Html.LabelFor(model => model.precio)
                </th>
                <th style="text-align:center"><i class="fa fa-camera-retro fa-lg"></i></th>
            </tr>
        </thead>
        <tbody></tbody>
    </table>
</div>
